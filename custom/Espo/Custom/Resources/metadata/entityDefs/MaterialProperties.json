            "layoutMassUpdateDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "isCustom": true
        },
        "valueType": {
            "type": "enum",
            "options": [
                "String",
                "Float",
                "Int"
            ],
            "style": {
                "String": null,
                "Float": null,
                "Int": null
            },
            "default": "String",
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "EntityTeam",
            "layoutRelationshipsDisabled": true
        },
        "materialClasses": {
            "type": "hasMany",
            "relationName": "materialClassesProperties",
